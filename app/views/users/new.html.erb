<h1>会員登録</h1>

<%= form_for :user, url: users_path do |f| %>
    <%= f.label  :name %> <br>
    <%= f.text_field  :name %><br>
    <%= f.label  :email %> <br>
    <%= f.text_field  :email %><br>
    <%= f.label  :password %> <br>
    <%= f.password_field  :password %><br>
    <%= f.label   :password_confirmation %><br>
    <%= f.password_field  :password_confirmation  %><br>
    <%= f.submit %><br>
<% end %>